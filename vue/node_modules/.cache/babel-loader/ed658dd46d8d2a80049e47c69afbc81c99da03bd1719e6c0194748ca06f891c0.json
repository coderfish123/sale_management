{"ast":null,"code":"export default {\n  data() {\n    let typeId = this.$route.query.id; //从路由拿到分类的id\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      typeId: typeId,\n      typeData: [],\n      goodsData: [],\n      //接收商品信息\n      recommendData: []\n    };\n  },\n  mounted() {\n    //渲染数据\n    this.loadGoods();\n    this.loadType();\n    this.loadRecommend();\n  },\n  methods: {\n    loadGoods() {\n      this.$request.get('/goods/selectByTypeId?id=' + this.typeId).then(res => {\n        if (res.code === '200') {\n          this.goodsData = res.data; //返回的数据res.data赋值给goodsData\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    loadType() {\n      this.$request.get('/type/selectById/' + this.typeId).then(res => {\n        if (res.code === '200') {\n          this.typeData = res.data;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    navTo(url) {\n      location.href = url;\n    },\n    loadRecommend() {\n      this.$request.get('/goods/recommend').then(res => {\n        if (res.code === '200') {\n          this.recommendData = res.data;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["data","typeId","$route","query","id","user","JSON","parse","localStorage","getItem","typeData","goodsData","recommendData","mounted","loadGoods","loadType","loadRecommend","methods","$request","get","then","res","code","$message","error","msg","navTo","url","location","href"],"sources":["src/views/front/Type.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"display:flex;height:500px;width: 70%;background-color:#fff;margin: 30px auto;border-radius: 20px\">\r\n      <div style=\"flex: 3;padding: 0 20px;\">\r\n        <div style=\"font-size: 18px; color: #000000FF; line-height: 80px; border-bottom: #cccccc 1px solid\">{{typeData.name}}</div>\r\n        <div style=\"margin: 20px 0;\">\r\n          <el-row :gutter=\"20\">\r\n            <el-col :span=\"6\" style=\"margin-bottom: 20px\" v-for=\"item in goodsData\">\r\n              <img @click=\"navTo('/front/detail?id=' + item.id)\" :src=\"item.img\" alt=\"\" style=\"width: 100%; height: 175px; border-radius: 10px; border: #cccccc 1px solid\">\r\n              <div style=\"margin-top: 10px; font-weight: 500; font-size: 16px; width: 180px; color: #000000FF; text-overflow: ellipsis; overflow: hidden; white-space: nowrap;\">{{item.name}}</div>\r\n              <div style=\"margin-top: 5px; font-size: 20px; color: #FF5000FF\">￥ {{item.price}} / {{item.unit}}</div>\r\n            </el-col>\r\n          </el-row>\r\n        </div>\r\n      </div>\r\n      <div style=\"width: 250px; padding: 0 20px; border-left: #cccccc 1px solid\" >\r\n        <div style=\"font-size: 18px; color: #000000FF; line-height: 80px; border-bottom: #cccccc 1px solid\">猜你喜欢</div>\r\n        <div style=\"margin: 20px 0; padding: 0 10px\">\r\n          <div style=\"margin-bottom: 20px\" v-for=\"item in recommendData\">\r\n            <img @click=\"navTo('/front/detail?id=' + item.id)\" :src=\"item.img\" alt=\"\" style=\"width: 100%; height: 175px; border-radius: 10px; border: #cccccc 1px solid\">\r\n            <div style=\"margin-top: 10px; font-weight: 500; font-size: 16px; width: 180px; color: #000000FF; text-overflow: ellipsis; overflow: hidden; white-space: nowrap;\">\r\n              {{ item.name }}</div>\r\n            <div style=\"margin-top: 5px; font-size: 20px; color: #FF5000FF\">{{ item.price }}元 / {{item.unit}}</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n\r\n\r\n  data() {\r\n    let typeId = this.$route.query.id//从路由拿到分类的id\r\n    return {\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      typeId: typeId,\r\n      typeData:[],\r\n      goodsData: [],//接收商品信息\r\n      recommendData:[]\r\n    }\r\n  },\r\n\r\n  mounted() {//渲染数据\r\n    this.loadGoods()\r\n    this.loadType()\r\n    this.loadRecommend()\r\n  },\r\n  methods:{\r\n    loadGoods(){\r\n      this.$request.get('/goods/selectByTypeId?id='+this.typeId).then(res=>{\r\n        if (res.code === '200'){\r\n          this.goodsData = res.data//返回的数据res.data赋值给goodsData\r\n        }else{\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    loadType() {\r\n      this.$request.get('/type/selectById/' + this.typeId).then(res => {\r\n        if (res.code === '200') {\r\n          this.typeData = res.data\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    navTo(url) {\r\n      location.href = url\r\n    },\r\n    loadRecommend() {\r\n      this.$request.get('/goods/recommend').then(res=>{\r\n        if (res.code === '200') {\r\n          this.recommendData = res.data\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    }\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AA+BA;EAGAA,KAAA;IACA,IAAAC,MAAA,QAAAC,MAAA,CAAAC,KAAA,CAAAC,EAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAR,MAAA,EAAAA,MAAA;MACAS,QAAA;MACAC,SAAA;MAAA;MACAC,aAAA;IACA;EACA;EAEAC,QAAA;IAAA;IACA,KAAAC,SAAA;IACA,KAAAC,QAAA;IACA,KAAAC,aAAA;EACA;EACAC,OAAA;IACAH,UAAA;MACA,KAAAI,QAAA,CAAAC,GAAA,oCAAAlB,MAAA,EAAAmB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAX,SAAA,GAAAU,GAAA,CAAArB,IAAA;QACA;UACA,KAAAuB,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;IACAV,SAAA;MACA,KAAAG,QAAA,CAAAC,GAAA,4BAAAlB,MAAA,EAAAmB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAZ,QAAA,GAAAW,GAAA,CAAArB,IAAA;QACA;UACA,KAAAuB,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;IACAC,MAAAC,GAAA;MACAC,QAAA,CAAAC,IAAA,GAAAF,GAAA;IACA;IACAX,cAAA;MACA,KAAAE,QAAA,CAAAC,GAAA,qBAAAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAV,aAAA,GAAAS,GAAA,CAAArB,IAAA;QACA;UACA,KAAAu